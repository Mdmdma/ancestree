# Example environment configuration for Ancestree
# Copy this file to .env and update with your actual values

# Google Maps API Key (required for map functionality)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# API Base URL - set this to your server's network IP for local network access
# For localhost development: http://localhost:3001/api
# For local network access: http://YOUR_IP_ADDRESS:3001/api (e.g., http://192.168.1.100:3001/api)
VITE_API_BASE_URL=http://localhost:3001/api
